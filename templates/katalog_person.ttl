@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix acdhi: <https://id.acdh.oeaw.ac.at/> .
@prefix acdh: <https://vocabs.acdh.oeaw.ac.at/schema#> .

{% for items in objects %}{% for md in items.metadata %}acdhi:{{ md.Vorname[0] | lower }}{{ md.Familienname | lower }} acdh:hasTitle "{{ md.Vorname }} {{ md.Familienname }}"@de;
a acdh:Person;
acdh:hasAlternativeTitle "{{ md.Vorname_gr }} {{ md.Familienname_gr }}"@mus;
{% if md.Vorname is string%}acdh:hasFirstName "{{ md.Vorname }}"@de,"{{ md.Vorname_gr }}"@mus;{% endif %}
{% if md.Familienname is string%}acdh:hasLastName "{{ md.Familienname }}"@de,"{{ md.Familienname_gr }}"@mus;{% endif %}
{% if md.Link1 is string%}acdh:hasIdentifier <{{ md.Link1 }}>;{% endif %}
{% if md.Link2 is string%}acdh:hasIdentifier <{{ md.Link2 }}>;{% endif %}
acdh:hasIdentifier <https://id.acdh.oeaw.ac.at/kretschmer-lesbos/{{ md.id }}> .{% endfor %}{% endfor %}
