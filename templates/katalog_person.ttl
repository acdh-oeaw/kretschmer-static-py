@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix acdhi: <https://id.acdh.oeaw.ac.at/> .
@prefix acdh: <https://vocabs.acdh.oeaw.ac.at/schema#> .

{% for items in objects %}{% for md in items.metadata %}
acdhi:{% if md.Vorname is string and md.Familienname is string %}{{ md.Vorname[0] | lower }}{{ md.Familienname | lower }}{% elif md.Vorname is string %}{{ md.Vorname.replace("[^a-zA-Z\d\s:]", "") | lower }}{% elif md.Familienname is string %}{{ md.Familienname.replace("[^a-zA-Z\d\s:]", "") | lower }}{% endif %}
acdh:hasTitle "{% if md.Vorname is string and md.Familienname is string %}{{ md.Vorname }} {{ md.Familienname }}{% elif md.Vorname is string %}{{ md.Vorname }}{% elif md.Familienname is string %}{{ md.Familienname }}{% endif %}"@de;
a acdh:Person;
acdh:hasAlternativeTitle "{% if md.VornameGr is string and md.FamiliennameGr is string %}{{ md.VornameGr }} {{ md.FamiliennameGr }}{% elif md.VornameGr is string %}{{ md.VornameGr }}{% elif md.FamiliennameGr is string %}{{ md.FamiliennameGr }}{% endif %}"@ell;
{% if md.Vorname is string%}acdh:hasFirstName "{{ md.Vorname }}"@de,"{{ md.Vorname_gr }}"@ell;{% endif %}
{% if md.Familienname is string%}acdh:hasLastName "{{ md.Familienname }}"@de,"{{ md.Familienname_gr }}"@ell;{% endif %}
{% if md.Link1 is string%}acdh:hasIdentifier <{{ md.Link1 }}>;{% endif %}
{% if md.Link2 is string%}acdh:hasIdentifier <{{ md.Link2 }}>;{% endif %}
acdh:hasIdentifier <https://id.acdh.oeaw.ac.at/kretschmer-lesbos/{{ md.id }}> .{% endfor %}{% endfor %}
